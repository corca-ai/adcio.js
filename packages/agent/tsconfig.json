{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2015",
    "strict": false,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "types": ["@emotion/react", "node"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true
  },
  "include": ["**/*.ts", "**/*.js", "**/*.tsx", "**/*.jsx"],
  "exclude": ["node_modules", "dist"]
}
