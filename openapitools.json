{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.6.0",
    "generators": {
      "controller": {
        "packageName": "controller",
        "generatorName": "typescript-axios",
        "output": "#{cwd}/packages/api/controller/v1",
        "inputSpec": "https://api.adcio.ai/api-json"
      },
      "receiver": {
        "packageName": "receiver",
        "generatorName": "typescript-axios",
        "output": "#{cwd}/packages/api/receiver/v1",
        "inputSpec": "https://receiver.adcio.ai/api-json"
      },
      "messenger": {
        "packageName": "messenger",
        "generatorName": "typescript-axios",
        "output": "#{cwd}/packages/api/messenger/v1",
        "inputSpec": "https://messenger.adcio.ai/api/v1/api/apis.json"
      }
    }
  }
}
