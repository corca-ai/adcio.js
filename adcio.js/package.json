  "author": "Corca <contact@corca.ai>",
  "license": "Apache-2.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/node": "^20.8.10",
    "babel-loader": "^9.1.3",
    "jsdom": "^23.0.1",
    "msw": "^2.0.10",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "prettier": "^3.0.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "tscpaths": "^0.0.9",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "mock-api": "workspace:*"
  },
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "gen-api": "openapi-generator-cli generate",
    "build": "tsc -p tsconfig.json && tscpaths -p tsconfig.json -s ./ -o ./dist && webpack --config webpack.config.js",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint \"**/*.ts\" --fix",
    "lint:ci": "eslint \"/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:cov": "vitest run --coverage"
  },
  "keywords": [],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "axios": "^1.6.1"
